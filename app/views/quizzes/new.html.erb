<h1>Create a quiz</h1>

<%= form_with model: @quiz do |form| %>
  <div class="form-group">
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div class="form-group">
    <%= form.label :author %><br>
    <%= form.text_field :author %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>